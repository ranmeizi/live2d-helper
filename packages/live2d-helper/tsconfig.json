{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./src",
    "paths": {
      "framework/*":["../../Framework/src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "../Core/*.ts"
  ]
}